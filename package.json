{
  "name": "@emperordev/structured-logger",
  "version": "0.1.0",
  "type": "module",
  "description": "Framework-agnostic structured logger for browser and node",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "example": "tsx src/examples/logger.ts"
  },
  "keywords": [
    "logger",
    "typescript",
    "structured-logging"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
